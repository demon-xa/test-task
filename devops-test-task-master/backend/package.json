{
  "name": "coffeeshop-backend",
  "version": "0.0.1",
  "description": "CoffeeShop App - Backend",
  "main": "src/server.ts",
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "migrations:dev": "pg-migrations apply --directory src/db/migrations",
    "migrations:prod": "pg-migrations apply --directory dist/db/migrations",
    "seeds:dev": "ts-node-dev -r tsconfig-paths/register src/seeds.ts",
    "seeds:prod": "node dist/seeds.js",
    "jobs:supply:dev": "ts-node-dev -r tsconfig-paths/register src/jobs/job.supply.ts",
    "jobs:supply:prod": "node dist/jobs/job.supply.js",
    "build": "tsc && tsc-alias",
    "server:prod": "node dist/server.js",
    "server:dev": "ts-node-dev --inspect=0.0.0.0:9229 --exit-child --respawn -r tsconfig-paths/register src/server.ts",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint **/*.ts --fix",
    "prettier": "prettier --check \"{*.js,!(node*)**/*.ts}\"",
    "prettier:fix": "prettier --write \"{*.js,!(node*)**/*.ts}\"",
    "loglevel:change": "runtime-node-refresh"
  },
  "dependencies": {
    "@databases/pg": "^5.5.0",
    "@databases/pg-migrations": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "apicache": "^1.6.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "express-prom-bundle": "^6.6.0",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "jest": "^27.0.0",
    "joi": "^17.5.0",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.11.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "prom-client": "^15.0.0",
    "runtime-node-refresh": "^1.0.6",
    "supertest": "^6.1.6",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.2",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.0",
    "@types/node": "^16.11.11",
    "@types/pg": "^7.14.4",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0"
  }
}
